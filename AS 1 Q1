#include<iostream>
using namespace std;

int main(){
    int i=0;
    while(i<5)
    {
        int space=5-i-1;
        if (i==0 || i==4)
        {
            cout<<"*****"<<endl;
        }
        else
        {
            while (space>0)
            {
            cout<<" ";
            space=space-1;
            }
            cout<<"*"<<endl;

        }
        i=i+1;
    }
    return 0;
}
