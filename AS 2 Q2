#include<iostream>
using namespace std;

int main(){
    cout<<"enter marks "<<endl;
    int n;
    cin>>n;
    if (n>90)
    {
        cout<<"exellent"<<endl;
    }
    else if (n>80 && n<=90)
    {
        cout<<"good"<<endl;
    }
    else if (n>70 && n<=80)
    {
        cout<<"fair"<<endl;
    }
    else if (n>60 && n<=70)
    {
        cout<<"meets expectations"<<endl;
    }
    else{
        cout<<"below par"<<endl;
    }
    return 0;
}
